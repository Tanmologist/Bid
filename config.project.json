{
    "name": "Task Mode",

    "tree": {
        "$className": "DataModel",
        "$ignoreUnknownInstances": true,

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "$ignoreUnknownInstances": true,
            
            "Library": {
                "$className": "Folder",
                "$path": "Library"
            },

            "Modules": {
                "$className": "Folder",
                
                "CameraShakeInstance": { "$path": "Modules/CameraShake/CameraShakeInstance.luau" },
                "CameraShakePresets": { "$path": "Modules/CameraShake/CameraShakePresets.luau" },
                "CameraShaker": { "$path": "Modules/CameraShake/CameraShaker.luau" },
                "characterHandlerLocal": { "$path": "Modules/CharacterControllers/characterHandlerLocal.luau" },
                "characterHandlerServer": { "$path": "Modules/CharacterControllers/characterHandlerServer.luau" }
            }
        },

        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$ignoreUnknownInstances": true,

            "Server": {
                "$className": "Folder",
                "$path": "Server"
            }
        },

        "StarterPlayer": {
            "$className": "StarterPlayer",
            "$ignoreUnknownInstances": true,

            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "$ignoreUnknownInstances": true,
                "$path": "StarterPlayerScripts",

                "Client": {
                    "$className": "Folder",
                    "$path": "Client"
                }
            },

            "StarterCharacterScripts": {
                "$className": "StarterCharacterScripts",
                "$ignoreUnknownInstances": true,
                "$path": "StarterCharacterScripts"
            }
        }
    }
}
